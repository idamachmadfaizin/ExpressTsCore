{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-airbnb"],
  "rules": {
    "quotemark": [true, "single"],
    "no-console": false,
    "curly": false,
    "import-name": false,
    "max-line-length": [
      true,
      {
        "limit": 200
      }
    ]
  },
  "rulesDirectory": []
}